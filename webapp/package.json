{
	"name": "timefox-application-client",
	"version": "0.1.0",
	"description": "Timefox Application Entity Gateways",
	"main": "dist/",
	"repository": "git@bitbucket.org:enbock-tecfox/timefox-application.git",
	"author": "Endre Bock <endre.bock@tecfox.eu>",
	"license": "MIT",
	"devDependencies": {
		"@testing-library/jest-dom": "^5.16.2",
		"@types/jest": "^27.4.0",
		"jest": "^27.4.7",
		"jest-mock-extended": "^2.0.4",
		"ts-jest": "^27.1.3",
		"typescript": "^4.5.5",
		"rimraf": "3.0.2",
		"@openapitools/openapi-generator-cli": "2.5.1"
	},
	"scripts": {
		"test": "jest",
		"clean-gen":"rimraf -rf src/generated-sources/timefoxapi",
		"gen-client-na": "openapi-generator-cli generate -i docs/client-na.yaml -g typescript-fetch -o generated-sources/timefoxapi"
	},
	"jest": {
		"roots": [
			"."
		],
		"collectCoverage": true,
		"verbose": true,
		"collectCoverageFrom": [
			"**/*.ts"
		],
		"coveragePathIgnorePatterns": [
			"TestHelper.ts"
		],
		"preset": "ts-jest",
		"testEnvironment": "jsdom",
		"testRegex": "\\.test\\.ts$",
		"moduleFileExtensions": [
			"js",
			"ts"
		],
		"coverageThreshold": {
			"global": {
				"branches": 90
			}
		},
		"modulePathIgnorePatterns": [
			".idea"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/setupTests.ts"
		]
	}
}
