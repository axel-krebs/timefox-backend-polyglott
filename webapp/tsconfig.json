{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "target": "es5",
	"lib": ["es6","dom"],
    "strict": true,
    "listEmittedFiles": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
	"esModuleInterop": true,
    "rootDir": "src/",
	"inlineSourceMap": true, 
    "outDir": "./dist"
  },
  "include": [
    "src/timefox-json.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
